set terminal postscript eps size 6.5,3.5 enhanced color font 'Arial,20' linewidth 2
set output "knn_metrics_weighted.eps"
set datafile separator ","
#set style data points
set ylabel "Accuracy"
set xlabel "K"
set yrange [0.8:1]
set xrange [1:15]
#set key left top
set style line 1 lt 1 lw 2 lc rgb '#332288' 
set style line 2 lt 3 lw 2 lc rgb '#88CCEE' 
set style line 3 lt 5 lw 2 lc rgb '#117733' 
set style line 4 lt 5 lw 2 lc rgb '#DDCC77' 
set style line 5 lt 5 lw 2 lc rgb '#CC6677' 
#set title "600 byte packet throughput"
plot 'knn_metrics_weighted.dat' using 1:2 title columnheader(2) ls 1 with lines, '' using 1:3 title columnheader(3) ls 2 with lines, '' using 1:4 title columnheader(4) ls 3 with lines, '' using 1:5 title columnheader(5) ls 4 with lines, '' using 1:6 title columnheader(6) ls 5 with lines 
