set terminal postscript eps size 6.5,3.5 enhanced color font 'Arial,20' linewidth 2
set output "fractional_weighted.eps"
set datafile separator ","
#set style data points
set ylabel "Accuracy"
set xlabel "Threshold T"
set yrange [0:1]
set xrange [0:1]
set key left top
set style line 1 lt 1 lw 2 lc rgb '#4477AA' 
set style line 2 lt 3 lw 2 lc rgb '#CC6677' 
set style line 3 lt 5 lw 2 lc rgb '#117733' 
#set title "600 byte packet throughput"
plot 'fractional_weighted.dat' using 1:2 title columnheader(2) ls 1 with lines, '' using 1:3 title columnheader(3) ls 2 with lines, '' using 1:4 title columnheader(4) ls 3 with lines
